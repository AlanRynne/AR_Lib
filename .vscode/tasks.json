{
  "version": "2.0.0",
  "tasks": [{
      "label": "build",
      "command": "dotnet",
      "type": "process",
      "args": [
        "build",
        "${workspaceFolder}/tests/Paramdigma.Core.Tests.csproj"
      ],
      "problemMatcher": []
    },
    {
      "label": "build-docs",
      "command": "docfx",
      "type": "shell",
      "args": [
        "docfx_project/docfx.json"
      ],
    },
    {
      "label": "serve-docs",
      "command": "docfx",
      "type": "shell",
      "args": [
        "docfx_project/docfx.json",
        "--serve"
      ],
      "isBackground": true,
      "problemMatcher": []
    },
  ]
}